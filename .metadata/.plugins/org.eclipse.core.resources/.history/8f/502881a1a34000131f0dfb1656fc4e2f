package p10_19;

public class Problem14_2 {

	public static void main(String args[])
	{
		int n = 2;
		int length = 1;
		while(true)
		{
			if((n - 1) % 3 == 0)
			{
				n = (n - 1) / 3;
				while(n > 10000)
				{
					n /= 2;
					length--;
					System.out.println(n);
					System.exit(0);
				}
				length++;
				System.out.println(n);
			}
			else
			{
				n = n * 2;
				length++;
				System.out.println(n);
			}
		}
		
	
	}
	
}
