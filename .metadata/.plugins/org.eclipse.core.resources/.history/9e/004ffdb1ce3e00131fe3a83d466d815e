
public class Problem3_2 {

	public static void main(String args[])
	{
		long num = 600851475143L;
		long sqr = (long) Math.sqrt(num);
		boolean flag = false;
		while(true)
		{
			flag = false;
			for(long i = 2; i <= sqr; i++)
			{
				if(num % i == 0)
				{
					num = num / i;
					flag = true;
					break;
				}
			}
			if(flag == false)
			{
				System.out.println(num);
				break;
			}
		}
	}
	
}
