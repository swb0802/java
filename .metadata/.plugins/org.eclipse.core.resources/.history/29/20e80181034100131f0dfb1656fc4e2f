package p10_19;

public class Problem15 {

	public static int calc(int x, int y)
	{
		if(x == 1 || y == 1)
		{
			return 1;
		}
		else if(x == y)
		{
			return 2 * calc(x-1, y);
		}
		else 
		{
			return calc(x-1, y) + calc(x, y-1);
		}
	}
	
	public static void main(String args[])
	{
		System.out.println(calc(1,1));
		System.out.println(calc(2,2));
		System.out.println(calc(3,3));
		System.out.println(calc(4,4));
		System.out.println(calc(5,5));
		System.out.println(calc(20,20));;
	}
	
}
