package p1_9;

public class Problem7_2 {

	public static boolean isPrime(int num)
	{
		if(num <= 4)
		{
			if(num == 2 || num == 3)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		int sqrNum = (int) Math.sqrt(num);
		for(int i = 5; i <= sqrNum; i += 6)
		{
			if(num % i == 0 || num % (i + 2)==0)
			{
				return false;
			}
		}
		return true;
	}
	
}
